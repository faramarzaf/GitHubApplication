<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/reposFragment">

    <fragment
        android:id="@+id/reposFragment"
        android:name="app.android.githubservice.ui.fragment.ReposFragment"
        android:label="fragment_repos"
        tools:layout="@layout/fragment_repos" >
        <action
            android:id="@+id/action_reposFragment_to_searchFragment2"
            app:destination="@id/searchFragment" />
        <action
            android:id="@+id/action_reposFragment_to_starredFragment"
            app:destination="@id/starredFragment" />
    </fragment>
    <fragment
        android:id="@+id/searchFragment"
        android:name="app.android.githubservice.ui.fragment.SearchFragment"
        android:label="fragment_search"
        tools:layout="@layout/fragment_search" >
        <action
            android:id="@+id/action_searchFragment_to_starredFragment"
            app:destination="@id/starredFragment" />
        <action
            android:id="@+id/action_searchFragment_to_reposFragment"
            app:destination="@id/reposFragment" />
    </fragment>
    <fragment
        android:id="@+id/starredFragment"
        android:name="app.android.githubservice.ui.fragment.StarredFragment"
        android:label="fragment_starred"
        tools:layout="@layout/fragment_starred" >
        <action
            android:id="@+id/action_starredFragment_to_searchFragment"
            app:destination="@id/searchFragment" />
        <action
            android:id="@+id/action_starredFragment_to_reposFragment"
            app:destination="@id/reposFragment" />
    </fragment>
</navigation>